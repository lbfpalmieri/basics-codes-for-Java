
public class Operadores {

	public static void main(String[] args) {
		
		double nota1 = 9.0;
		double nota2 = 10;
		
		double media = 0; 
		
		media = (nota1+nota2)/2;
		
		System.out.println(media);
		
		int valor = 15;
		System.out.println(valor/2);
		System.out.println(valor%2);
		
		if(valor%2 == 1) {
			double x;
			x = valor;
			System.out.println(x);
		}
		else {
			System.out.println("Divisao com numero multiplo de 2");
		}

	}

}
